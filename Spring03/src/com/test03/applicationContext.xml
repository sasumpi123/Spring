<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hong" class="com.test03.Developer">
		<constructor-arg name="name" value="홍길동"></constructor-arg>
		<constructor-arg name="salary" value="1000"></constructor-arg>
		<property name="dept" value="어딘가"></property>
	</bean>
	
	<bean id="lee" class="com.test03.Developer">
		<constructor-arg name="name" value="이순신"></constructor-arg>
		<constructor-arg name="salary" value="1000"></constructor-arg>
		<property name="dept" value="어디엔가"></property>
	</bean>
	
</beans>
